  public final int yylength() {
    return zzMarkedPos-zzStartRead;
  }


  /**
   * Reports an error that occured while scanning.
   *
   * In a wellformed scanner (no or only correct usage of 
   * yypushback(int) and a match-all fallback rule) this method 
   * will only be called with things that "Can't Possibly Happen".
   * If this method is called, something is seriously wrong
   * (e.g. a JFlex bug producing a faulty scanner etc.).
   *
   * Usual syntax/scanner level error handling should be done
   * in error fallback rules.
   *
   * @param   errorCode  the code of the errormessage to display
   */
  private void zzScanError(int errorCode) {
    String message;
    try {
      message = ZZ_ERROR_MSG[errorCode];
    }
    catch (ArrayIndexOutOfBoundsException e) {
      message = ZZ_ERROR_MSG[ZZ_UNKNOWN_ERROR];
    }

    throw new Error(message);
  } 


  /**
   * Pushes the specified amount of characters back into the input stream.
   *
   * They will be read again by then next call of the scanning method
   *
   * @param number  the number of characters to be read again.
   *                This number must not be greater than yylength()!
   */
  public void yypushback(int number)  {
    if ( number > yylength() )
      zzScanError(ZZ_PUSHBACK_2BIG);

    zzMarkedPos -= number;
  }


  /**
   * Contains user EOF-code, which will be executed exactly once,
   * when the end of file is reached
   */
  private void zzDoEOF() throws java.io.IOException {
    if (!zzEOFDone) {
      zzEOFDone = true;
      yyclose();
    }
  }


  /**
   * Resumes scanning until the next regular expression is matched,
   * the end of input is encountered or an I/O-Error occurs.
   *
   * @return      the next token
   * @exception   java.io.IOException  if any I/O-Error occurs
   */
  public java_cup.runtime.Symbol next_token() throws java.io.IOException {
    int zzInput;
    int zzAction;

    // cached fields:
    int zzCurrentPosL;
    int zzMarkedPosL;
    int zzEndReadL = zzEndRead;
    char [] zzBufferL = zzBuffer;
    char [] zzCMapL = ZZ_CMAP;

    int [] zzTransL = ZZ_TRANS;
    int [] zzRowMapL = ZZ_ROWMAP;
    int [] zzAttrL = ZZ_ATTRIBUTE;

    while (true) {
      zzMarkedPosL = zzMarkedPos;

      boolean zzR = false;
      for (zzCurrentPosL = zzStartRead; zzCurrentPosL < zzMarkedPosL;
                                                             zzCurrentPosL++) {
        switch (zzBufferL[zzCurrentPosL]) {
        case '\u000B':
        case '\u000C':
        case '\u0085':
        case '\u2028':
        case '\u2029':
          yyline++;
          yycolumn = 0;
          zzR = false;
          break;
        case '\r':
          yyline++;
          yycolumn = 0;
          zzR = true;
          break;
        case '\n':
          if (zzR)
            zzR = false;
          else {
            yyline++;
            yycolumn = 0;
          }
          break;
        default:
          zzR = false;
          yycolumn++;
        }
      }

      if (zzR) {
        // peek one character ahead if it is \n (if we have counted one line too much)
        boolean zzPeek;
        if (zzMarkedPosL < zzEndReadL)
          zzPeek = zzBufferL[zzMarkedPosL] == '\n';
        else if (zzAtEOF)
          zzPeek = false;
        else {
          boolean eof = zzRefill();
          zzEndReadL = zzEndRead;
          zzMarkedPosL = zzMarkedPos;
          zzBufferL = zzBuffer;
          if (eof) 
            zzPeek = false;
          else 
            zzPeek = zzBufferL[zzMarkedPosL] == '\n';
        }
        if (zzPeek) yyline--;
      }
      zzAction = -1;

      zzCurrentPosL = zzCurrentPos = zzStartRead = zzMarkedPosL;
  
      zzState = ZZ_LEXSTATE[zzLexicalState];


      zzForAction: {
        while (true) {
    
          if (zzCurrentPosL < zzEndReadL)
            zzInput = zzBufferL[zzCurrentPosL++];
          else if (zzAtEOF) {
            zzInput = YYEOF;
            break zzForAction;
          }
          else {
            // store back cached positions
            zzCurrentPos  = zzCurrentPosL;
            zzMarkedPos   = zzMarkedPosL;
            boolean eof = zzRefill();
            // get translated positions and possibly new buffer
            zzCurrentPosL  = zzCurrentPos;
            zzMarkedPosL   = zzMarkedPos;
            zzBufferL      = zzBuffer;
            zzEndReadL     = zzEndRead;
            if (eof) {
              zzInput = YYEOF;
              break zzForAction;
            }
            else {
              zzInput = zzBufferL[zzCurrentPosL++];
            }
          }
          int zzNext = zzTransL[ zzRowMapL[zzState] + zzCMapL[zzInput] ];
          if (zzNext == -1) break zzForAction;
          zzState = zzNext;

          int zzAttributes = zzAttrL[zzState];
          if ( (zzAttributes & 1) == 1 ) {
            zzAction = zzState;
            zzMarkedPosL = zzCurrentPosL;
            if ( (zzAttributes & 8) == 8 ) break zzForAction;
          }

        }
      }

      // store back cached position
      zzMarkedPos = zzMarkedPosL;

      switch (zzAction < 0 ? zzAction : ZZ_ACTION[zzAction]) {
        case 34: 
          { return new Symbol(SintaxisLenguajesSym.tab, yycolumn, yyline, yytext());
          }
        case 46: break;
        case 38: 
          { return new Symbol(SintaxisLenguajesSym.autor, yycolumn, yyline, yytext());
          }
        case 47: break;
        case 20: 
          { return new Symbol(SintaxisLenguajesSym.menos, yycolumn, yyline, yytext());
          }
        case 48: break;
        case 36: 
          { /*NO PRESTAR ATENCION*/
          }
        case 49: break;
        case 11: 
          { return new Symbol(SintaxisLenguajesSym.llavec, yycolumn, yyline, yytext());
          }
        case 50: break;
        case 1: 
          { return new Symbol(SintaxisLenguajesSym.codigo, yycolumn, yyline, yytext());
          }
        case 51: break;
        case 27: 
          { return new Symbol(SintaxisLenguajesSym.id, yycolumn, yyline, yytext());
          }
        case 52: break;
        case 13: 
          { return new Symbol(SintaxisLenguajesSym.coma, yycolumn, yyline, yytext());
          }
        case 53: break;
        case 33: 
          { return new Symbol(SintaxisLenguajesSym.lin, yycolumn, yyline, yytext());
          }
        case 54: break;
        case 37: 
          { return new Symbol(SintaxisLenguajesSym.str, yycolumn, yyline, yytext());
          }
        case 55: break;
        case 26: 
          { return new Symbol(SintaxisLenguajesSym.idt, yycolumn, yyline, yytext());
          }
        case 56: break;
        case 17: 
          { return new Symbol(SintaxisLenguajesSym.parentesisa, yycolumn, yyline, yytext());
          }
        case 57: break;
        case 45: 
          { return new Symbol(SintaxisLenguajesSym.lanzamiento, yycolumn, yyline, yytext());
          }
        case 58: break;
        case 15: 
          { return new Symbol(SintaxisLenguajesSym.corchetea, yycolumn, yyline, yytext());
          }
        case 59: break;
        case 8: 
          { return new Symbol(SintaxisLenguajesSym.mult, yycolumn, yyline, yytext());
          }
        case 60: break;
        case 19: 
          { return new Symbol(SintaxisLenguajesSym.mas, yycolumn, yyline, yytext());
          }
        case 61: break;
        case 31: 
          { return new Symbol(SintaxisLenguajesSym.pord, yycolumn, yyline, yytext());
          }
        case 62: break;
        case 18: 
          { return new Symbol(SintaxisLenguajesSym.parentesisc, yycolumn, yyline, yytext());
          }
        case 63: break;
        case 16: 
          { return new Symbol(SintaxisLenguajesSym.corchetec, yycolumn, yyline, yytext());
          }
        case 64: break;
        case 23: 
          { return new Symbol(SintaxisLenguajesSym.comilla, yycolumn, yyline, yytext());
          }
        case 65: break;
        case 40: 
          { return new Symbol(SintaxisLenguajesSym.nom, yycolumn, yyline, yytext());
          }
        case 66: break;
        case 22: 
          { return new Symbol(SintaxisLenguajesSym.dosp, yycolumn, yyline, yytext());
          }
        case 67: break;
        case 25: 
          { return new Symbol(SintaxisLenguajesSym.punto, yycolumn, yyline, yytext());
          }
        case 68: break;
        case 30: 
          { return new Symbol(SintaxisLenguajesSym.pp, yycolumn, yyline, yytext());
          }
        case 69: break;
        case 44: 
          { return new Symbol(SintaxisLenguajesSym.extension, yycolumn, yyline, yytext());
          }
        case 70: break;
        case 39: 
          { return new Symbol(SintaxisLenguajesSym.version, yycolumn, yyline, yytext());
          }
        case 71: break;
        case 4: 
          { return new Symbol(SintaxisLenguajesSym.caracterma, yycolumn, yyline, yytext());
          }
        case 72: break;
        case 41: 
          { return new Symbol(SintaxisLenguajesSym.result, yycolumn, yyline, yytext());
          }
        case 73: break;
        case 32: 
          { return new Symbol(SintaxisLenguajesSym.dpp, yycolumn, yyline, yytext());
          }
        case 74: break;
        case 28: 
          { return new Symbol(SintaxisLenguajesSym.idn, yycolumn, yyline, yytext());
          }
        case 75: break;
        case 12: 
          { return new Symbol(SintaxisLenguajesSym.percent, yycolumn, yyline, yytext());
          }
        case 76: break;
        case 10: 
          { return new Symbol(SintaxisLenguajesSym.y, yycolumn, yyline, yytext());
          }
        case 77: break;
        case 5: 
          { return new Symbol(SintaxisLenguajesSym.number, yycolumn, yyline, yytext());
          }
        case 78: break;
        case 14: 
          { return new Symbol(SintaxisLenguajesSym.igual, yycolumn, yyline, yytext());
          }
        case 79: break;
        case 7: 
          { return new Symbol(SintaxisLenguajesSym.div, yycolumn, yyline, yytext());
          }
        case 80: break;
        case 24: 
          { return new Symbol(SintaxisLenguajesSym.pre, yycolumn, yyline, yytext());
          }
        case 81: break;
        case 35: 
          { return new Symbol(SintaxisLenguajesSym.esp, yycolumn, yyline, yytext());
          }
        case 82: break;
        case 2: 
          { /*Ignore*/
          }
        case 83: break;
        case 43: 
          { return new Symbol(SintaxisLenguajesSym.term, yycolumn, yyline, yytext());
          }
        case 84: break;
        case 9: 
          { return new Symbol(SintaxisLenguajesSym.llavea, yycolumn, yyline, yytext());
          }
        case 85: break;
        case 42: 
          { return new Symbol(SintaxisLenguajesSym.printf, yycolumn, yyline, yytext());
          }
        case 86: break;
        case 29: 
          { return new Symbol(SintaxisLenguajesSym.no, yycolumn, yyline, yytext());
          }
        case 87: break;
        case 21: 
          { return new Symbol(SintaxisLenguajesSym.puntocoma, yycolumn, yyline, yytext());
          }
        case 88: break;
        case 3: 
          { return new Symbol(SintaxisLenguajesSym.caracterme, yycolumn, yyline, yytext());
          }
        case 89: break;
        case 6: 
          { return new Symbol(SintaxisLenguajesSym.or, yycolumn, yyline, yytext());
          }
        case 90: break;
        default: 
          if (zzInput == YYEOF && zzStartRead == zzCurrentPos) {
            zzAtEOF = true;
            zzDoEOF();
              { return new java_cup.runtime.Symbol(SintaxisLenguajesSym.EOF); }
          } 
          else {
            zzScanError(ZZ_NO_MATCH);
          }
      }
    }
  }


